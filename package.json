{
  "name": "gep",
  "version": "0.0.2",
  "description": "General Expression Parser",
  "main": "dist/gep.js",
  "scripts": {
    "test": "$(npm bin)/mocha",
    "cover": "$(npm bin)/istanbul cover _mocha -- -R spec",
    "lint": "$(npm bin)/eslint src/**",
    "dev": "node dev/watch",
    "build": "rm -rf dist && npm run rollup && MIN=min npm run rollup",
    "rollup": "$(npm bin)/rollup --config --environment NODE_ENV:production --sourcemap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/longhaohe/gep.git"
  },
  "keywords": [
    "gep"
  ],
  "author": "longhao he <longhaohe@gmail.com> (http://longhaohe.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/longhaohe/gep/issues"
  },
  "homepage": "https://github.com/longhaohe/gep#readme",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "chai": "^3.5.0",
    "eslint": "^2.10.2",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.0",
    "eslint-plugin-standard": "^1.3.2",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.3",
    "rollup": "^0.26.3",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-uglify": "^0.3.1"
  }
