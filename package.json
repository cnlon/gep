{
  "name": "gep",
  "version": "1.3.1",
  "description": "General Expression Parser",
  "main": "dist/gep.js",
  "scripts": {
    "test": "$(npm bin)/mocha",
    "cover": "$(npm bin)/istanbul cover _mocha -- -R spec",
    "lint": "$(npm bin)/eslint src/**",
    "dev": "node dev/watch",
    "build": "rm -rf dist && npm run rollup && MIN=min npm run rollup",
    "rollup": "$(npm bin)/rollup --config --environment NODE_ENV:production --sourcemap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnlon/gep.git"
  },
  "keywords": [
    "gep"
  ],
  "author": "longhao he <longhaohe@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cnlon/gep/issues"
  },
  "homepage": "https://github.com/cnlon/gep#readme",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "chai": "^3.5.0",
    "eslint": "^3.10.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.3.2",
    "eslint-plugin-standard": "^2.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-uglify": "^1.0.1"
  }
}
